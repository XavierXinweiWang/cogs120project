<body class="mdl-demo mdl-color--grey-100 mdl-color-text--grey-700 mdl-base">
<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
    <header class="mdl-layout__header mdl-layout__header--scroll blue-grey lighten-5" id="header" style="height: 48px">
        <div class="mdl-layout__tab-bar blue-grey lighten-5" id="toolbar" style="height: 48px">
            <a href="/home" class=" mdl-layout__tab" id="arrow-back"><i class="material-icons" style="padding-top: 5vw">arrow_back</i></a>
            <h2 href="#compare" class="mdl-layout__header-row">Compare Similarity</h2>
        </div>
    </header>
    <main class="mdl-layout__content">

        <div class="mdl-layout__tab-panel is-active" id="compare">
            <div>
                <form class="col s12">
                    <div class="row">
                        <div class="col s8">
                            <h7>Be a wise buyer!</h7>
                        </div>
                        <div class="col s1"></div>
                        <div class="col s3">
                            <a href="/signout" id="logout"><h7>Logout</h7></a>
                        </div>
                    </div>
                </form>
            </div>
            <div class="row" id="data1">
                <div class="col s12 m7">
                    <div class="card">
                        <div>
                            <div class="mdl-card mdl-cell--12-col">
                                <form class="col s12">
                                    <div class="row">
                                        <div class="card-image" id="upload">
                                            <img class="icon icons8-Upload-to-Cloud" src="/images/cloud_upload_grey_192x192.png" id="uploadIcon">
                                            <img src="/images/rockmui_aloneteewhite-1000x1000.jpg" id="fakeUpload">
                                        </div>
                                    </div>
                                </form>
                            </div>

                            <div class="row" style="display: none" id="itemsToCompare">
                                {{#each clothes-min}}
                                    <div class="input-field col l3 m3 s3">
                                        <img src="{{imageURL}}" class="fakeitem2 tocompare">
                                    </div>
                                {{/each}}
                            </div>


                        </div>

                        <div class="card-action">
                            <h7 id="uploadInstruction">Upload the cloth you want to buy</h7>
                            <div id="resultPercentage">
                                <h5 style="display: none">Similarity: 50%</h5>
                                <div class="row" id="resultButtons">
                                    <a class="waves-effect waves-light btn col s6 red" type="submit" id="cancel">Cancel</a>
                                    <a href="/home" class="waves-effect waves-light btn col s6" type="submit" id="addCloth">Buy It!</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </main>

</div>


</body>
<script src="https://code.getmdl.io/1.2.1/material.min.js"></script>
<script src="javascripts/indexAnimation.js" type="text/javascript"></script>
</html>
