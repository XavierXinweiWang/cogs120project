<body class="mdl-demo mdl-color--grey-100 mdl-color-text--grey-700 mdl-base">
<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
    <header class="mdl-layout__header mdl-layout__header--scroll blue-grey lighten-5" id="header">
        <div class="mdl-layout__header-row" id="topbar">
            <h1 id="app-name">Vestiarium</h1>
            <a href="/compare" class=" mdl-layout__tab mdl-js-ripple-effect" id="to-upload"><i class="material-icons" style="padding-top: 2.5vh">camera_alt</i></a>
        </div>
        <div class="mdl-layout__tab-bar mdl-js-ripple-effect blue-grey lighten-5" id="toolbar">
            <a href="#history" class="mdl-layout__tab" id="to-saved-looks">Saved Looks</a>
            <a href="#closet" class="mdl-layout__tab is-active" id="to-all-clothes">All Clothes</a>
            <a href="#explore" class="mdl-layout__tab" id="to-explore">Explore</a>
        </div>
    </header>
    <main class="mdl-layout__content">
        <div class="mdl-layout__tab-panel" id="history">
            <div>
                <form class="col s12">
                    <div class="row">
                        <div class="col s8">
                          <div>
                            <h7>Here are Your Saved Looks</h7>
                          </div>
                          <div>
                            <h7>Click each entry for more info!</h7>
                          </div>
                        </div>
                        <div class="col s3">
                          <a class="mdl-button mdl-js-button mdl-button--accent" href="/signout" id="logout">Logout</a>
                        </div>
                    </div>
                </form>
            </div>
        <ul class="collapsible" data-collapsible="accordion" id="collection">
            {{#each saved-looks}}
            <li id="data1">
                <div class="collapsible-header">
                    <section class="section--center mdl-grid mdl-grid--no-spacing">
                        <div class="mdl-card mdl-cell--12-col">
                            <div class="mdl-card mdl-cell mdl-cell--12-col" id="form">
                                <form class="col s12">
                                    <div class="row">
                                        <div class="input-field col l6 m6 s6">
                                            <img src="{{imageURL1}}" class="fakeitem1" id="fake1">
                                        </div>

                                        <div class="input-field col l6 m6 s6">
                                            <p>{{title}}</p>
                                            <p id="lastwear1"><i class="material-icons">access_time</i>{{date}}</p>
                                            <p id="like1"><i class="material-icons">favorite</i>{{like}} likes</p>
                                        </div>

                                    </div>
                                </form>
                            </div>
                        </div>
                    </section>
                </div>

                <div class="collapsible-body">
                    <section class="section--center mdl-grid mdl-grid--no-spacing">
                        <div class="mdl-card mdl-cell--12-col">
                            <div class="mdl-card mdl-cell mdl-cell--12-col">
                                <form class="col s12">
                                    <div class="row">
                                        <div class="input-field col l3 m3 s3"></div>
                                        <div class="input-field col l3 m3 s3" id = "fakedata1a">
                                            <img src="{{imageURL1}}" class="fakeitem2 onview">
                                        </div>

                                        <div class="input-field col l3 m3 s3" id = "fakedata1b">
                                            <img src="{{imageURL2}}" class="fakeitem2">
                                        </div>

                                        <div class="input-field col l6 m6 s6">
                                          <a class="mdl-button mdl-js-button mdl-button--accent delete" style="width: 100%" id="delete1">DELETE</a>
                                        </div>
                                        <div class="input-field col l6 m6 s6">
                                          <a class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent wear" style="width: 100%" id="wear1">WEAR</a>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </section>
                </div>
            </li>
            {{/each}}
        </ul>


        </div>


        <div class="mdl-layout__tab-panel is-active" id="closet">
            <div>
                <form class="col s12">
                    <div class="row">
                        <div class="col s8">
                          <div>
                            <h7>View all your clothes here</h7>
                          </div>
                          <div>
                            <h7>Select to make your new looks!</h7>
                          </div>
                        </div>
                        <div class="col s3">
                          <a class="mdl-button mdl-js-button mdl-button--accent" href="/signout" id="logout">Logout</a>
                        </div>
                    </div>
                </form>
            </div>
            <div id="top" class="card">
                <h5>&nbsp;&nbsp;Top</h5>
                <ul class="top list">
                    <div class="demo-grid-ruler mdl-grid" id="topGrid">
                      {{#each clothes}}
                      {{#if top}}
                        <div class="row">
                            <div class="col">
                                <div class="card z-depth-0">
                                    <div class="card-image">
                                        <p style="display: none">{{title}}</p>
                                        <img src="{{imageURL}}">
                                        <i class="material-icons check-icon" style="display: none">check_circle</i>
                                    </div>
                                </div>
                            </div>
                        </div>
                      {{/if}}
                      {{/each}}
                    </div>
                </ul>
            </div>


            <div id="bottom" class="card">
                <h5>&nbsp;&nbsp;Bottom</h5>
                <ul class="bottom list">
                    <div class="demo-grid-ruler mdl-grid">
                      {{#each clothes}}
                      {{#if bottom}}
                        <div class="row">
                            <div class="col">
                                <div class="card z-depth-0">
                                    <div class="card-image">
                                        <p style="display: none">{{title}}</p>
                                        <img src="{{imageURL}}">
                                        <i class="material-icons check-icon" style="display: none">check_circle</i>
                                    </div>
                                </div>
                            </div>
                        </div>
                      {{/if}}
                      {{/each}}
                    </div>
                </ul>
            </div>


            <div id="one-piece" class="card">
                <h5>&nbsp;&nbsp;One Piece</h5>
                <ul class="one-piece list">
                    <div class="demo-grid-ruler mdl-grid">
                      {{#each clothes}}
                      {{#if one-piece}}
                        <div class="row">
                            <div class="col">
                                <div class="card z-depth-0">
                                    <div class="card-image">
                                        <p style="display: none">{{title}}</p>
                                        <img src="{{imageURL}}">
                                        <i class="material-icons check-icon" style="display: none">check_circle</i>
                                    </div>
                                </div>
                            </div>
                        </div>
                      {{/if}}
                      {{/each}}
                    </div>
                </ul>
            </div>
        </div>


        <div class="mdl-layout__tab-panel" id="explore">
            <div>
                <form class="col s12">
                    <div class="row">
                        <div class="col s8">
                          <div>
                            <h7>View other users' looks</h7>
                          </div>
                          <div>
                            <h7>Give it a like if you like!</h7>
                          </div>
                        </div>
                        <div class="col s3">
                          <a class="mdl-button mdl-js-button mdl-button--accent" href="/signout" id="logout">Logout</a>
                        </div>
                    </div>
                </form>
            </div>
            {{#if bversion}}
            {{#each exploration}}
          <div class="row" id="data1">
            <div class="col s12 m7">
              <div class="card">
                <div>
                              <form class="col s12">
                                  <div class="row">
                                      <div class="input-field col">
                                          <img src="{{imageURL1}}" class="fakeitem1 explore" id="fake1">
                                      </div>
                                  </div>
                              </form>


                                    <div class="row">
                                        <div class="input-field col l2 m2 s2"></div>
                                        <div class="input-field col l3 m3 s3" id = "fakedata1a">
                                            <img src="{{imageURL1}}" class="fakeitem2 onview">
                                        </div>
                                        <div class="input-field col l2 m2 s2"></div>
                                        <div class="input-field col l3 m3 s3" id = "fakedata1b">
                                            <img src="{{imageURL2}}" class="fakeitem2">
                                        </div>
                                        <div class="input-field col l2 m2 s2"></div>
                                        <div class="input-field col l12 m12 s12" id = "fakedataDescription">
                                            <h5>{{ title }}</h5>
                                            <h7>Liked by {{like}} People!</h7>
                                        </div>
                                    </div>

                </div>
                <div class="card-action">
                  <a class="waves-effect waves-light btn skip A-Version pink accent-2"><i class="material-icons left">shuffle</i>NEXT</a>
                  <a class="waves-effect waves-light btn before-like pink accent-2"><i class="material-icons left">favorite_border</i>LIKE</a>
                </div>
              </div>
            </div>
          </div>
          {{/each}}
          {{else}}
          {{#each exploration}}
              <div class="row" id="data1">
                  <div class="col s12 m7">
                      <div class="card">
                          <div>
                                  <form class="col s12">
                                      <div class="row">
                                          <div class="input-field col">
                                              <img src="{{imageURL1}}" class="fakeitem1 explore" id="fake1">
                                          </div>
                                      </div>
                                  </form>

                              <a class="btn-floating btn-small alternateNext B-Version pink accent-2"><i class="material-icons skip">trending_flat</i></a>

                              <div class="row">
                                  <div class="input-field col l2 m2 s2"></div>
                                  <div class="input-field col l3 m3 s3" id = "fakedata1a">
                                      <img src="{{imageURL1}}" class="fakeitem2 onview">
                                  </div>
                                  <div class="input-field col l2 m2 s2"></div>
                                  <div class="input-field col l3 m3 s3" id = "fakedata1b">
                                      <img src="{{imageURL2}}" class="fakeitem2">
                                  </div>
                                  <div class="input-field col l2 m2 s2"></div>
                                  <div class="input-field col l12 m12 s12" id = "fakedataDescription">
                                      <h5>{{ title }}</h5>
                                      <h7>Liked by {{like}} People!</h7>
                                      <br>
                                  </div>
                              </div>

                          </div>
                          <div class="card-action">
                            <a class="waves-effect waves-light btn pink accent-2" style="visibility: hidden"><i class="material-icons left">shuffle</i>NEXT</a>
                              <a class="waves-effect waves-light btn before-like pink accent-2"><i class="material-icons left">favorite_border</i>LIKE</a>
                          </div>
                      </div>
                  </div>
              </div>
          {{/each}}
          {{/if}}
        </div>

    </main>

</div>


<div id="wear-snackbar" class="mdl-js-snackbar mdl-snackbar">
  <div class="mdl-snackbar__text">You have already worn it today.</div>
  <button class="mdl-snackbar__action" type="button"></button>
</div>


<div class="fixed-action-btn vertical" style="bottom: 5%; right: 5%; display: none;">
    <div class="btn-floating btn-large pink accent-2">
        <i class="large material-icons" id="filter">done</i>
    </div>
</div>
</body>
<script src="https://code.getmdl.io/1.2.1/material.min.js"></script>
<script src="javascripts/indexAnimation.js" type="text/javascript"></script>
</html>
